<h1><%= @title %></h1>

<a href="/meetup/new">Add a new meetup!</a>

<table>
  <th></th>
  <th>User Name</th>
  <th>User Email</th>
  <% @users.each do |user| %>
    <tr>
      <td><img src="<%= user.avatar_url %>" width=30 height=30></td>
      <td><a href="/users/<%= user.id %>"><%= user.username %></a></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>
